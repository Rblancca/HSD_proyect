<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/fcv_activity_menu"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <com.hsd.contest.spain.component.bottomview.MyBottomNavigationView
        android:id="@+id/mbnv_activity_menu"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:layout_gravity="bottom"
        app:contentInsetStart="0dp"
        app:endIcon="@drawable/ic_baseline_directions_run_24"
        app:startIcon="@drawable/ic_baseline_cloud_queue_24" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/fab_activity_menu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:backgroundTint="#111"
        app:layout_anchor="@id/mbnv_activity_menu"
        app:srcCompat="@drawable/ic_baseline_home_24"
        app:tint="@android:color/white" />

</androidx.coordinatorlayout.widget.CoordinatorLayout>